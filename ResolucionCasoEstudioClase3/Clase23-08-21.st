Object subclass: #Alumno	instanceVariableNames: 'legajo nombre nota1 nota2'	classVariableNames: ''	package: 'Clase23-08-21'!!Alumno methodsFor: 'converting' stamp: 'Ana 8/23/2021 17:28'!asString		^ 'Legajo: ', self legajo asString, ' - Nombre: ', self nombre asString,		' - Nota1: ', self nota1 asString, ' - Nota2: ', self nota2 asString.		! !!Alumno methodsFor: 'negocio' stamp: 'Ana 8/23/2021 17:33'!promedio	"calcula el promedio de las dos notas"					| promedio |		"esto me da un promedio entero, no me sirve	promedio := self nota1 + self nota2 / 2."		promedio := (self nota1 + self nota2) asFloat / 2.0.		^ promedio.! !!Alumno methodsFor: 'initialization' stamp: 'Ana 8/23/2021 17:26'!initialize 	legajo := 0.	nombre := ''.	nota1 := 0.	nota2 := 0.	! !!Alumno methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:25'!nota1	^ nota1! !!Alumno methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:25'!nota1: anObject	nota1 := anObject! !!Alumno methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:25'!legajo	^ legajo! !!Alumno methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:25'!nombre: anObject	nombre := anObject! !!Alumno methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:25'!nota2	^ nota2! !!Alumno methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:25'!nombre	^ nombre! !!Alumno methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:25'!nota2: anObject	nota2 := anObject! !!Alumno methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:25'!legajo: anObject	legajo := anObject! !Object subclass: #Curso	instanceVariableNames: 'codigo turno docente alumnos'	classVariableNames: ''	package: 'Clase23-08-21'!!Curso methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:13'!codigo	^ codigo! !!Curso methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:13'!turno	^ turno! !!Curso methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:13'!docente: anObject	docente := anObject! !!Curso methodsFor: 'accessing' stamp: 'Ana 8/23/2021 17:13'!turno: anOb