Object subclass: #Mantenimiento	instanceVariableNames: 'sucursal trabajos'	classVariableNames: ''	poolDictionaries: ''	category: 'PrimerParcial1C2019'!!Mantenimiento methodsFor: 'as yet unclassified' stamp: 'Ana 5/3/2019 11:01'!primerTrabajoProgramado	| trabajo mensaje |	mensaje := ''.		trabajo := trabajos detect: [ :unTrabajo | (unTrabajo class = TrabajoProgramado) & (unTrabajo costoTrabajo < self costoPromedio ) ] ifNone: [ trabajo := nil].		(trabajo isNotNil) ifTrue: [ mensaje := trabajo asString ] 							ifFalse: [ mensaje := 'No encontrado' ].		^trabajo .	! !!Mantenimiento methodsFor: 'as yet unclassified' stamp: 'Ana 5/3/2019 10:38'!costoPromedio	| suma cantidad promedio|	suma := 0.0.	cantidad := (trabajos size) asFloat .		trabajos do: [ :unTrabajo | suma := suma + unTrabajo costoTrabajo asFloat ].		promedio := suma / cantidad. 		^promedio .	! !!Mantenimiento methodsFor: 'as yet unclassified' stamp: 'Ana 5/3/2019 10:42'!costoComprendido: unValor y: otroValor	| coleccionAux  listado |	listado := ''.		coleccionAux := trabajos select: [ :unTrabajo | unTrabajo costoTrabajo between: unValor and: otroValor ].		(coleccionAux isNotNil) ifTrue: [ coleccionAux do: [ :unTrabajo | listado := listado, unTrabajo asString ] ].		^listado .	! !!Mantenimiento methodsFor: 'initialization' stamp: 'Ana 5/3/2019 10:20'!initialize	super initialize.	sucursal := ''.	trabajos := OrderedCollection new.! !!Mantenimiento methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:21'!add: unTrabajo	trabajos add: unTrabajo .! !!Mantenimiento methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:21'!sucursal: anObject	sucursal := anObject! !!Mantenimiento methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:21'!trabajos: anObject	trabajos := anObject! !!Mantenimiento methodsFor: 'accessing' stamp: 'Ana 5/3/2019 13:50'!asString	| listado |	listado := ''.	self trabajos do: [ :unTrabajo | listado := listado , unTrabajo asString  ].	^String cr, ' Sucursal: ', self sucursal asString , ', Productos: ', String cr, listado.	! !!Mantenimiento methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:21'!trabajos	^ trabajos! !!Mantenimiento methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:21'!sucursal	^ sucursal! !Object subclass: #Tarea	instanceVariableNames: 'numero descripcion horasTotales precioHora'	classVariableNames: ''	poolDictionaries: ''	category: 'PrimerParcial1C2019'!!Tarea methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:07'!precioHora	^ precioHora! !!Tarea methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:07'!descripcion: anObject	descripcion := anObject! !!Tarea methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:07'!numero	^ numero! !!Tarea methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:07'!numero: anObject	numero := anObject! !!Tarea methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:07'!descripcion	^ descripcion! !!Tarea methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:07'!horasTotales: anObject	horasTotales := anObject! !!Tarea methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:07'!horasTotales	^ horasTotales! !!Tarea methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:11'!asSring	^String cr, 'Número: ', self numero asString, ', Descripción: ', self descripcion asString,	     ', Horas totales: ', self horasTotales asString, ', Precio por hora: ', self precioHora asString. ! !!Tarea methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:07'!precioHora: anObject	precioHora := anObject! !!Tarea methodsFor: 'initialization' stamp: 'Ana 5/3/2019 10:07'!initialize	super initialize.	descripcion := nil.	horasTotales := nil.	numero := nil.	precioHora := nil.! !Object subclass: #Trabajo	instanceVariableNames: 'numero descripcion fecha cantidadHoras'	classVariableNames: ''	poolDictionaries: ''	category: 'PrimerParcial1C2019'!!Trabajo methodsFor: 'initialization' stamp: 'Ana 5/3/2019 10:07'!initialize	super initialize.	cantidadHoras := nil.	descripcion := nil.	fecha := nil.	numero := nil.! !!Trabajo methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!descripcion: anObject	descripcion := anObject! !!Trabajo methodsFor: 'accessing' stamp: 'Ana 5/3/2019 14:01'!asString	^String cr, 'Número: ', self numero asString, ', Descripción: ', self descripcion asString,	     ', Cantidad horas: ', self cantidadHoras asString, ', Fecha: ', self fecha asString.! !!Trabajo methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!fecha: anObject	fecha := anObject! !!Trabajo methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!cantidadHoras	^ cantidadHoras! !!Trabajo methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!numero: anObject	numero := anObject! !!Trabajo methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!descripcion	^ descripcion! !!Trabajo methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!fecha	^ fecha! !!Trabajo methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:31'!costoTrabajo	^self subclassResponsibility .! !!Trabajo methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!numero	^ numero! !!Trabajo methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!cantidadHoras: anObject	cantidadHoras := anObject! !Trabajo subclass: #TrabajoNoProgramado	instanceVariableNames: 'precioHora viaticos'	classVariableNames: ''	poolDictionaries: ''	category: 'PrimerParcial1C2019'!!TrabajoNoProgramado methodsFor: 'as yet unclassified' stamp: 'Ana 5/3/2019 10:32'!costoTrabajo	| costo |	costo := (self cantidadHoras asFloat * self precioHora asFloat) + self viaticos asFloat .	^costo .! !!TrabajoNoProgramado methodsFor: 'initialization' stamp: 'Ana 5/3/2019 10:08'!initialize	super initialize.	precioHora := nil.	viaticos := nil.! !!TrabajoNoProgramado methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!viaticos: anObject	viaticos := anObject! !!TrabajoNoProgramado methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!precioHora	^ precioHora! !!TrabajoNoProgramado methodsFor: 'accessing' stamp: 'Ana 5/3/2019 14:06'!asString	^super asString, ', Precio por hora : ', self precioHora asString,	     ', Viaticos: ', self viaticos asString, '', ', Costo trabajo: ', self costoTrabajo asString.! !!TrabajoNoProgramado methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!viaticos	^ viaticos! !!TrabajoNoProgramado methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!precioHora: anObject	precioHora := anObject! !Trabajo subclass: #TrabajoProgramado	instanceVariableNames: 'tarea'	classVariableNames: ''	poolDictionaries: ''	category: 'PrimerParcial1C2019'!!TrabajoProgramado methodsFor: 'initialization' stamp: 'Ana 5/3/2019 10:08'!initialize	super initialize.	tarea := nil.! !!TrabajoProgramado methodsFor: 'as yet unclassified' stamp: 'Ana 5/3/2019 10:30'!costoTrabajo	| costo |	(self cantidadHoras < self tarea horasTotales ) 			ifTrue: [ costo := self cantidadHoras asFloat * (self tarea precioHora) asFloat ]			ifFalse: [ costo := self tarea horasTotales asFloat * (self tarea precioHora) asFloat  ].	^costo .! !!TrabajoProgramado methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!tarea: anObject	tarea := anObject! !!TrabajoProgramado methodsFor: 'accessing' stamp: 'Ana 5/3/2019 14:06'!asString	^super asString, ', Tarea : ', self tarea asString, ', Costo trabajo: ', self costoTrabajo asString.! !!TrabajoProgramado methodsFor: 'accessing' stamp: 'Ana 5/3/2019 10:08'!tarea	^ tarea! !