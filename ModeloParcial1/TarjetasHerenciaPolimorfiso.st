Object subclass: #EntidadFinanciera	instanceVariableNames: 'nombre listaTarjetas'	classVariableNames: ''	package: 'TarjetasHerenciaPolimorfiso'!!EntidadFinanciera methodsFor: 'negocio' stamp: 'silvioserra 9/2/2021 21:56'!sumaSaldosCredito2	|suma listaAux|	suma := 0.	listaAux := listaTarjetas  select:[:unaTarjeta| unaTarjeta isKindOf: TarjetaCredito].	listaAux do: [:unaTarjeta | suma := suma + unaTarjeta saldo].	^suma.! !!EntidadFinanciera methodsFor: 'negocio' stamp: 'silvio 6/12/2014 14:28'!initialize	listaTarjetas := Bag new.! !!EntidadFinanciera methodsFor: 'negocio' stamp: 'silvio 6/12/2014 14:29'!addTarjeta: unaTarjeta	listaTarjetas  add: unaTarjeta! !!EntidadFinanciera methodsFor: 'negocio' stamp: 'silvioserra 9/2/2021 21:52'!depositar: unValor numero: unNumero	listaTarjetas  do:[:unaTarjeta| 		((unaTarjeta numero) = unNumero)			ifTrue: [^unaTarjeta depositar: unValor]].	^false.! !!EntidadFinanciera methodsFor: 'negocio' stamp: 'silvio 9/11/2015 16:03'!asString	|str|	str:='Entidad Financiera ' , nombre asString ,' Sus tarjetas('. 		listaTarjetas do:[:unaTarjeta| str:= str , unaTarjeta asString, String cr].		str:=str ,  ')'.	^str! !!EntidadFinanciera methodsFor: 'negocio' stamp: 'silvio 9/11/2015 16:14'!extraer: unValor numero: unNumero	listaTarjetas  do:[:unaTarjeta| 		((unaTarjeta numero) = unNumero)			ifTrue: [^(unaTarjeta extraer: unValor)]].	^false	! !!EntidadFinanciera methodsFor: 'negocio' stamp: 'Anonymous 9/14/2018 14:57'!listarSaldos	|listaAux listaString|	listaString := ''.	listaAux := listaTarjetas  asSortedCollection:[:x :y | x saldo < y saldo].	listaAux do:[:unaTarjeta | listaString := listaString , unaTarjeta asString , String cr].	^listaString.! !!EntidadFinanciera methodsFor: 'negocio' stamp: 'silvioserra 9/2/2021 21:56'!sumaSaldosCredito	|suma|	suma := 0.	listaTarjetas  do:[:unaTarjeta| (unaTarjeta isKindOf: TarjetaCredito) ifTrue: [suma := suma + unaTarjeta saldo]].	^suma.! !!EntidadFinanciera methodsFor: 'acceso' stamp: 'silvio 6/12/2014 14:23'!nombre	^ nombre.! !!EntidadFinanciera methodsFor: 'acceso' stamp: 'silvio 6/12/2014 14:24'!nombre: unNombre	nombre := unNombre.! !Object subclass: #Tarjeta	instanceVariableNames: 'numero titular saldo'	classVariableNames: ''	package: 'TarjetasHerenciaPolimorfiso'!!Tarjeta methodsFor: 'acceso' stamp: 'silvio 6/12/2014 02:08'!numero	^numero! !!Tarjeta methodsFor: 'acceso' stamp: 'silvio 6/12/2014 02:07'!titular	^ titular! !!Tarjeta methodsFor: 'acceso' stamp: 'silvio 6/12/2014 02:08'!saldo	^saldo! !!Tarjeta methodsFor: 'acceso' stamp: 'silvio 6/12/2014 02:27'!saldo: unSaldo	saldo := unSaldo.! !!Tarjeta methodsFor: 'acceso' stamp: 'silvio 6/12/2014 02:26'!numero: unNumero	numero := unNumero.! !!Tarjeta methodsFor: 'acceso' stamp: 'silvio 6/12/2014 02:27'!titular: unTitular	titular := unTitular.! !!Tarjeta methodsFor: 'negocio' stamp: 'silvio 6/12/2014 02:28'!asString	^ 'Numero: ' , numero asString , ' Saldo: ' , saldo asString , ' Titular: ' , titular asString.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Tarjeta class	instanceVariableNames: 'numero saldo titular'!Tarjeta subclass: #TarjetaCredito	instanceVariableNames: 'limite'	classVariableNames: ''	package: 'TarjetasHerenciaPolimorfiso'!!TarjetaCredito methodsFor: 'acceso' stamp: 'silvio 6/12/2014 02:42'!limite: unLimite	limite := unLimite.! !!TarjetaCredito methodsFor: 'acceso' stamp: 'silvio 6/12/2014 02:42'!limite 	^limite.! !!TarjetaCredito methodsFor: 'negocio' stamp: 'silvio 9/11/2015 15:54'!extraer: unValor	(saldo + unValor) <= limite ifTrue: [ saldo := saldo + unValor. ^true ]								ifFalse:[^false].! !!TarjetaCredito methodsFor: 'negocio' stamp: 'silvio 6/12/2014 02:43'!asString	^ super asString , ' Limite: ' , limite asString.! !!TarjetaCredito methodsFor: 'negocio' stamp: 'silvioserra 9/2/2021 21:50'!depositar: unValor	(saldo - unValor) >= 0 ifTrue: [ saldo := saldo - unValor. ^true ]								  ifFalse: [ ^false ].! !Tarjeta subclass: #TarjetaDebito	instanceVariableNames: 'banco'	classVariableNames: ''	package: 'TarjetasHerenciaPolimorfiso'!!TarjetaDebito methodsFor: 'acceso' stamp: 'silvio 6/12/2014 02:11'!banco	^banco.! !!TarjetaDebito methodsFor: 'acceso' stamp: 'silvio 6/12/2014 02:26'!banco: unBanco	banco := unBanco.! !!TarjetaDebito methodsFor: 'negocio' stamp: 'silvio 9/11/2015 15:57'!extraer: unValor	(saldo - unValor) >= 0 ifTrue: [ saldo := saldo - unValor. ^true]						    ifFalse: [^false]. ! !!TarjetaDebito methodsFor: 'negocio' stamp: 'silvio 6/12/2014 02:28'!asString	^ super asString , ' Banco: ' , banco asString.! !!TarjetaDebito methodsFor: 'negocio' stamp: 'silvioserra 9/2/2021 21:48'!depositar: unValor	saldo := saldo + unValor.	^true.! !